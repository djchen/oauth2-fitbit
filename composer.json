{
    "name": "djchen/oauth2-fitbit",
    "description": "Fitbit OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Chen",
            "email": "dan@djc.me",
            "homepage": "https://www.djc.me"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authentication",
        "fitbit"
    ],
    "require": {
        "php": ">=5.4.0",
        "league/oauth2-client": "~1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0",
        "fabpot/php-cs-fixer": "~1.0",
        "jakub-onderka/php-parallel-lint": "0.8.*"
    },
    "autoload": {
        "psr-4": {
            "djchen\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "djchen\\OAuth2\\Client\\Test\\": "tests/src/"
        }
    }
}
